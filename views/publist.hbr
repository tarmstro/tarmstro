<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Tom Armstrong &mdash; Publications</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Tom Armstrong - assistant professor of computer science">
    <meta name="author" content="Tom Armstrong - assistant professor of computer science">
    <link rel="shortcut icon" href="public/img/favicon.png">
    <script type="text/javascript">//<![CDATA[
    // Google Analytics for WordPress by Yoast v4.2.8 | http://yoast.com/wordpress/google-analytics/
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-71379-7']);
            _gaq.push(['_trackPageview']);
    (function () {
      var ga = document.createElement('script');
      ga.type = 'text/javascript';
      ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0];
      s.parentNode.insertBefore(ga, s);
    })();
    //]]></script>
    <!-- Bootstrap core CSS -->
    <link href="../public/css/font-awesome/css/font-awesome.min.css" rel="stylesheet"/>
    <link href="../public/css/bootstrap.css" rel="stylesheet">
    <style>
        mark {background:#ff950a;color:#fff;padding:5px 3px 6px;white-space:nowrap}
        em {background:#00ba48;color:#fff;font-style:normal;padding:1px 2px 2px;white-space:nowrap}
    </style>
</head>
<body>
<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <font size="12">Publications</font>
        </div>
    </div>


    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <table class="table table-striped">
                {{#each papers}}
                    <tr>
                        <td>
                            <a href="#{{this.filename}}">{{this.title}}</a>
                        </td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>


    {{#each papers}}
        <a id="{{filename}}"></a>
        <div class="row">
            <div class="col-md-8">
                <h2>{{this.title}}</h2>
                <p class="lead" style="text-indent:10px;">by {{{this.friendly_author}}}</p>
                <blockquote>
                  <p class="text-info">{{this.abstract}}</p>
                  <footer>{{booktitle}}, {{year}}</footer>
                </blockquote>
            </div>
            <div class="col-md-4">
                <br/><br/><br/>
                <center>
                    <a href="../public/docs/{{filename}}.pdf"><img src="../public/docs/{{filename}}.jpg" alt="PDF download of {{title}}" class="img-thumbnail" width=250px></a>
                    <br/><br/>
                    <p>[<a href="http://dx.doi.org/{{doi}}"><b>DOI</b></a>] [<a href="../public/docs/{{filename}}.bib"><b>BibTeX</b></a>]</p>
                </center>
            </div>
        </div>
        <div class="col-md-6 col-md-offset-3">
            <hr>
        </div>
    {{/each}}
</div>

      <div class="footer" align="center">
        <p>&copy; Tom Armstrong 2014</p>
      </div>


</body>
</html>